{
  "name": "yiddishe-kop/user-preferences",
  "description": "Easily store preferences per user",
  "keywords": [
    "yiddishe-kop",
    "laravel",
    "user preferences"
  ],
  "homepage": "https://github.com/Yiddishe-Kop/UserPreferences",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Yehuda Neufeld",
      "email": "yehuda@yiddishe-kop.com",
      "role": "Developer"
    }
  ],
  "require": {
    "illuminate/support": "^7.0|^8.71|^9.0|^10.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.13",
    "orchestra/testbench": "~5|~6",
    "phpunit/phpunit": "~9.0"
  },
  "autoload": {
    "psr-4": {
      "YiddisheKop\\UserPreferences\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "YiddisheKop\\UserPreferences\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "YiddisheKop\\UserPreferences\\UserPreferencesServiceProvider"
      ],
      "aliases": {
        "UserPreferences": "YiddisheKop\\UserPreferences\\Facades\\UserPreferences"
      }
    }
  }
}
